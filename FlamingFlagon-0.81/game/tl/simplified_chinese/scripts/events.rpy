# game/scripts/events.rpy:4706
translate simplified_chinese bathhouse_odachi_end_bcac6470:

    # Niall grin "{size=80}Cannonball!{/size}"
    Niall grin "{size=80}肉身飞弹!{/size}"

# game/scripts/events.rpy:4719
translate simplified_chinese bathhouse_odachi_end_57e7638b:

