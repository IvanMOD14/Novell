    # w "\"그래...\"" id c05_w_0701
    w "\"Ага...\"" id c05_w_0701

# game/script/chapter5_wolf.rpy:2850
translate russian c05_w_0702:

    # "가헬은 흥분과 부끄러움이 뒤섞인 얼굴로 말했다." id c05_w_0702
    "Ответил Gahel, и на его лице отразилась смесь возбуждения и смущения." id c05_w_0702

# game/script/chapter5_wolf.rpy:2855
