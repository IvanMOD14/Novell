    # w "\"... 고맙다.\"" id c05_w_0597
    w "\"...Спасибо.\"" id c05_w_0597

# game/script/chapter5_wolf.rpy:2443
translate russian c05_w_0598:

    # "[pl_name][josa_i_ga] 다른 대화 주제를 꺼냈다." id c05_w_0598
    "[pl_name] затронул другую тему." id c05_w_0598

# game/script/chapter5_wolf.rpy:2445
translate russian c05_w_0599:

    # c question2 "\"가헬은 왜 용병이 되기로 했어? 역시 어릴 때부터 재능이 있었나?\"" id c05_w_0599
    c question2 "\"Почему ты решил стать наемником, Gahel? Был ли ты талантлив от природы с юных лет?\"" id c05_w_0599

# game/script/chapter5_wolf.rpy:2450
translate russian c05_w_0600:

    # "가헬은 살짝 멈칫했다. [pl_name][josa_eun_neun] 정말로 과거의 가헬을 기억하지 못하는 모양이다." id c05_w_0600
    "Gahel на мгновение заколебался. Похоже, [pl_name] действительно ничего не помнит о прошлом Gahel." id c05_w_0600

# game/script/chapter5_wolf.rpy:2455
translate russian c05_w_0601:

    # w "\"비슷하지만 조금 다르다. 어릴 때부터 가족이 없어서, 당장 먹고살기에 급했거든.\"" id c05_w_0601
    w "\"Это близко, но немного по-другому. Когда я рос, у меня не было семьи, поэтому я просто пытался выжить изо дня в день.\"" id c05_w_0601

# game/script/chapter5_wolf.rpy:2457
translate russian c05_w_0602:

    # w "\"어쩌면 농부나 광부가 되었을지도 모르지.\"" id c05_w_0602
    w "\"Может быть, я бы стал фермером или шахтером.\"" id c05_w_0602

# game/script/chapter5_wolf.rpy:2462
translate russian c05_w_0603:

    # c talk "\"그랬구나. 나는... 비교적 상황이 좋았네.\"" id c05_w_0603
    c talk "\"Понял. Думаю, мне относительно повезло.\"" id c05_w_0603

# game/script/chapter5_wolf.rpy:2464
translate russian c05_w_0604:

    # "[pl_name][josa_eun_neun] 류호를 떠올리며 기억을 되짚었다." id c05_w_0604
    "[pl_name] вернулся мыслями к Ryu-ho, вспоминая его воспоминания." id c05_w_0604

# game/script/chapter5_wolf.rpy:2466
translate russian c05_w_0605:

    # c "\"한 12년 정도 전에 스승님을 만나서 지낼 곳이 생겼지.\"" id c05_w_0605
    c "\"Около 12 лет назад я встретил своего учителя, и именно тогда у меня наконец появилось место, где я мог бы остановиться.\"" id c05_w_0605

# game/script/chapter5_wolf.rpy:2468
translate russian c05_w_0606:

    # c sad_am "\"아무튼 스승님은 거의 항상 여행 중이라서... 혼자였던 날이 훨씬 많았지만.\"" id c05_w_0606
    c sad_am "\"Хотя, честно говоря, мой учитель почти все время был в разъездах... так что большую часть времени я проводил в одиночестве.\"" id c05_w_0606

# game/script/chapter5_wolf.rpy:2470
translate russian c05_w_0607:

    # "가헬은 처음 듣는 [pl_name]의 과거 얘기에 귀를 기울였다." id c05_w_0607
    "Gahel внимательно слушал, как [pl_name] впервые поделился своим прошлым." id c05_w_0607

# game/script/chapter5_wolf.rpy:2472
translate russian c05_w_0608:

    # c talk "\"스승님한테 배운 것으로 뭘 할 수 있을까 생각하다가, 마법 상점을 차리게 된 거야.\"" id c05_w_0608
    c talk "\"Я начал думать о том, что я мог бы сделать с тем, чему научился у своего мастера, и в итоге открыл магический магазин.\"" id c05_w_0608

# game/script/chapter5_wolf.rpy:2477
translate russian c05_w_0609:

    # w "\"... 스승님을 만나기 전에는?\"" id c05_w_0609
    w "\"...А что было до того, как ты встретил своего учителя?\"" id c05_w_0609

# game/script/chapter5_wolf.rpy:2482
translate russian c05_w_0610:

    # "가헬의 질문에 [pl_name][josa_eun_neun] 잠시 생각에 빠졌다." id c05_w_0610
    "[pl_name] замолчал, глубоко задумавшись." id c05_w_0610

# game/script/chapter5_wolf.rpy:2484
translate russian c05_w_0611:

    # c question2 "\"그 전에?...\"" id c05_w_0611
    c question2 "\"До этого?...\"" id c05_w_0611

# game/script/chapter5_wolf.rpy:2485
translate russian c05_w_0612:

    # c talk "\"글쎄, 기억이 잘 안 나는데.\"" id c05_w_0612
    c talk "\"Ну, на самом деле я почти ничего не помню.\"" id c05_w_0612

# game/script/chapter5_wolf.rpy:2487
translate russian c05_w_0613:

    # "열심히 생각해 보려고 해도 머릿속에 떠오르는 게 거의 없었다." id c05_w_0613
    "Как бы я ни старался думать, в голову почти ничего не приходило." id c05_w_0613

# game/script/chapter5_wolf.rpy:2488
translate russian c05_w_0614:

    # "아주 단편적인 순간만이 남아있었다." id c05_w_0614
    "Осталось всего несколько разрозненных мгновений." id c05_w_0614

# game/script/chapter5_wolf.rpy:2493
translate russian c05_w_0615:

    # w "\"뭔가 더 어릴 때의 기억은 없나?\"" id c05_w_0615
    w "Есть ли у тебя какие-нибудь воспоминания о том времени, когда ты были моложе?" id c05_w_0615

# game/script/chapter5_wolf.rpy:2498
translate russian c05_w_0616:

    # c consider "\"어릴 때는 들판에서 뛰어다니고... 수영도 하고...\"" id c05_w_0616
    c consider "\"Когда я был ребенком, я часто бегал по полям... поплавал...\"" id c05_w_0616

# game/script/chapter5_wolf.rpy:2500
translate russian c05_w_0617:

    # "가헬은 [pl_name]에게 보육원에 대해 말해야 할지 고민했다." id c05_w_0617
    "Gahel заколебался, раздумывая, стоит ли ему упоминать о сиротском приюте." id c05_w_0617

# game/script/chapter5_wolf.rpy:2501
translate russian c05_w_0618:

    # "함께 놀았던 어린 시절을 [pl_name][josa_i_ga] 기억하지 못하는 게 너무 아쉬웠다." id c05_w_0618
    "Ему было грустно, что [pl_name] не может вспомнить их совместное детство." id c05_w_0618

# game/script/chapter5_wolf.rpy:2502
translate russian c05_w_0619:

    # "하지만 괜히 안 좋은 기억까지 건드리고 싶지는 않았다." id c05_w_0619
    "Но он не хотел говорить ни о чем, что могло бы пробудить плохие воспоминания." id c05_w_0619

# game/script/chapter5_wolf.rpy:2507
translate russian c05_w_0620:

    # w "(조심스럽게 물어볼까...)" id c05_w_0620
    w "(Должен ли я осторожно спросить...)" id c05_w_0620

# game/script/chapter5_wolf.rpy:2512
translate russian c05_w_0621:

    # w "\"보육원에서 지내던 시절은... 기억나지 않나?\"" id c05_w_0621
    w "\"Ты помнишь... жил в приюте?\"" id c05_w_0621

# game/script/chapter5_wolf.rpy:2519
translate russian c05_w_0622:

    # c question2 "\"보육원?...\"" id c05_w_0622
    c question2 "\"Приют?...\"" id c05_w_0622

# game/script/chapter5_wolf.rpy:2521
translate russian c05_w_0623:

    # c talk "\"그, 그렇네? 나는 아버지들이 없으니까, 보육원에서 컸는데.\"" id c05_w_0623
    c talk "\"Ну, д-да, это так. У меня не было родителей, поэтому я вырос в детском доме.\"" id c05_w_0623

# game/script/chapter5_wolf.rpy:2566
translate russian c05_w_0624:

    # c consider "\"왜... 잘 기억이 안 나지?\"" id c05_w_0624
    c consider "\"Почему... я не могу хорошо это запомнить?\"" id c05_w_0624

# game/script/chapter5_wolf.rpy:2568
translate russian c05_w_0625:

    # "분명 보육원에서 지내던 기억이 있는데, 자세히 떠올리려고 노력할수록 그 어떤 것도 기억나지 않았다." id c05_w_0625
    "У [pl_name] определенно были воспоминания о жизни в приюте, но чем больше он пытался вспомнить их в деталях, тем больше все расплывалось." id c05_w_0625

# game/script/chapter5_wolf.rpy:2569
translate russian c05_w_0626:

    # "물건을 같이 쓰는 게 싫었는데, 어떤 물건을 누구와 같이 썼는지 모르겠다." id c05_w_0626
    "Он помнил, что не любил делиться чем-то, но не мог вспомнить, что это были за вещи и с кем он ими делился." id c05_w_0626

# game/script/chapter5_wolf.rpy:2570
translate russian c05_w_0627:

    # "[pl_name][josa_eun_neun] 텅 비어버린 기억에 기묘한 느낌이 들었다." id c05_w_0627
    "Пустота в его воспоминаниях казалась странной." id c05_w_0627

# game/script/chapter5_wolf.rpy:2572
translate russian c05_w_0628:

    # c "(마치 책 위에 잉크를 엎은 것 같은... 아니, 페이지가 찢어진 책 같은...)" id c05_w_0628
    c "(Это все равно, что пролить чернила на книгу... или, может быть, вырвать из нее страницы...)" id c05_w_0628

# game/script/chapter5_wolf.rpy:2600
translate russian c05_w_0629:

    # w "\"[pl_name]? [pl_name]! 괜찮나? 무리해서 생각할 필요 없다.\"" id c05_w_0629
    w "\"[pl_name]? [pl_name]! Ты в порядке? Нет необходимости заставлять себя вспоминать.\"" id c05_w_0629

# game/script/chapter5_wolf.rpy:2607
translate russian c05_w_0630:

    # "가헬은 [pl_name][josa_i_ga] 보육원의 사고를 떠올리고 충격을 받은 건 아닌지 걱정했다." id c05_w_0630
    "Gahel беспокоился, что [pl_name], возможно, был шокирован, вспомнив несчастный случай в приюте." id c05_w_0630

# game/script/chapter5_wolf.rpy:2609
translate russian c05_w_0631:

    # c talk "\"응? 난 괜찮아.\"" id c05_w_0631
    c talk "\"А? Я в порядке.\"" id c05_w_0631

# game/script/chapter5_wolf.rpy:2611
translate russian c05_w_0632:

    # "[pl_name][josa_eun_neun] 왜 가헬의 표정이 심각한지 이해하지 못했다." id c05_w_0632
    "[pl_name] не понимал, почему Gahel выглядел таким обеспокоенным." id c05_w_0632

# game/script/chapter5_wolf.rpy:2616
translate russian c05_w_0633:

    # w "\"... 잊어버렸다면 상관없다.\"" id c05_w_0633
    w "\"...Если ты забыл, то все в порядке.\"" id c05_w_0633

# game/script/chapter5_wolf.rpy:2621
translate russian c05_w_0634:

    # "[pl_name][josa_eun_neun] 가헬의 반응이 신경 쓰였다." id c05_w_0634
    "Но реакция Gahel осталась на [pl_name]." id c05_w_0634

# game/script/chapter5_wolf.rpy:2622
translate russian c05_w_0635:

    # "아쉬움과 안도감이 뒤섞인 복잡한 표정 때문에 [pl_name]도 생각이 많아졌다." id c05_w_0635
    "Смесь сожаления и облегчения на лице Gahel заставила [pl_name] задуматься." id c05_w_0635

# game/script/chapter5_wolf.rpy:2624
translate russian c05_w_0636:

    # c laugh "\"어디 크게 다친 적 없는 거 같으니, 잘 지낸 거겠지?\"" id c05_w_0636
    c laugh "\"Если я не помню, чтобы я серьезно пострадал, значит, все было хорошо?\"" id c05_w_0636

# game/script/chapter5_wolf.rpy:2629
translate russian c05_w_0637:

    # "[pl_name][josa_eun_neun] 아무렇지 않게 말했지만, 속으로는 다른 생각을 했다." id c05_w_0637
    "[pl_name] говорил небрежно, но внутри он думал о чем-то другом." id c05_w_0637

# game/script/chapter5_wolf.rpy:2631
translate russian c05_w_0638:

    # c base "(가헬은 뭔가 알고 있어. 지금 당장은 숨기고 싶어 하지만...)" id c05_w_0638
    c base "(Gahel что-то знает. Он хочет пока сохранить это в тайне...)" id c05_w_0638

# game/script/chapter5_wolf.rpy:2636
translate russian c05_w_0639:

    # w "\"훈련하느라 피곤했을 텐데, 괜한 소리를 했군.\"" id c05_w_0639
    w "\"Ты, должно быть, устал от всех этих тренировок. Мне не следовало поднимать эту тему.\"" id c05_w_0639

# game/script/chapter5_wolf.rpy:2641
translate russian c05_w_0640:

    # c talk "\"아니야. 별로 안 피곤해.\"" id c05_w_0640
    c talk "\"Нет, я не настолько устал.\"" id c05_w_0640

# game/script/chapter5_wolf.rpy:2642
translate russian c05_w_0641:

    # c ddam2 "\"아... 설마 가게까지 뛰어갈 생각은 아니지?\"" id c05_w_0641
    c ddam2 "\"Ах... Ты же не собираешься бежать обратно в магазин, правда?\"" id c05_w_0641

# game/script/chapter5_wolf.rpy:2644
translate russian c05_w_0642:

    # "[pl_name][josa_eun_neun] 더 이상 훈련을 하고 싶지 않았다." id c05_w_0642
    "У [pl_name] не было желания продолжать обучение." id c05_w_0642

# game/script/chapter5_wolf.rpy:2645
translate russian c05_w_0643:

    # "하지만 가헬의 대답은 예상하던 것과 전혀 달랐다." id c05_w_0643
    "Но ответ Gahel был совершенно неожиданным." id c05_w_0643

# game/script/chapter5_wolf.rpy:2650
translate russian c05_w_0644:

    # w "\"피곤하지 않으면... 오늘 밤은 기대해도 되나?\"" id c05_w_0644
    w "\"Если ты не устал... значит ли это, что я могу с нетерпением ждать сегодняшнего вечера?\"" id c05_w_0644

# game/script/chapter5_wolf.rpy:2656
translate russian c05_w_0645:

    # c ddam "\"!!...\"" id c05_w_0645
    c ddam "\"!!...\"" id c05_w_0645

# game/script/chapter5_wolf.rpy:2658
translate russian c05_w_0646:

