# game/script/chapter5_wolf.rpy:33
translate russian c05_w_0003:

    # c talk "\"안녕.\"" id c05_w_0003
    c talk "\"Доброе утро.\"" id c05_w_0003

# game/script/chapter5_wolf.rpy:38
translate russian c05_w_0004:

    # w "\"잘 잤나.\"" id c05_w_0004
    w "\"Ты хорошо спал?\"" id c05_w_0004

# game/script/chapter5_wolf.rpy:43
translate russian c05_w_0005:

