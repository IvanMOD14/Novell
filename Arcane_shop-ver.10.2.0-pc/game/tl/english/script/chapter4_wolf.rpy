    # l "\"안녕하십니까, 가헬님...\"" id c04_w_0200
    l "\"Hello, Sir Gahel...\"" id c04_w_0200

# game/script/chapter4_wolf.rpy:805
translate english c04_w_0201:

    # l "\"!!...\"" id c04_w_0201
    l "\"!!...\"" id c04_w_0201

