# game/script/chapter5_wolf.rpy:1929
translate chinese_simple c05_w_0475:

    # "[pl_name][josa_eun_neun] 놀란 표정으로 대답했다." id c05_w_0475
    "[pl_name]惊讶地回答道。" id c05_w_0475

# game/script/chapter5_wolf.rpy:1931
translate chinese_simple c05_w_0476:

    # c ddam2 "\"그게 보통이라고? 휘두르면 몸이 휘청이는 거 아냐?\"" id c05_w_0476
